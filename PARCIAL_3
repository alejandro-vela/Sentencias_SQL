#----PUNTO 2----#
SELECT *FROM JUGADORES
WHERE FECHA_NACIMIENTO_JUGADOR < '1998-00-00' ORDER BY FECHA_NACIMIENTO_JUGADOR ASC;

#----PUNTO 3----#
SELECT CODIGO_JUGADOR,NOMBRE_JUGADOR,POSICION FROM JUGADORES;

#----PUNTO 4----#
SELECT NOMBRE_JUGADOR,POSICION, CODIGO_EQUIPO,FECHA_NACIMIENTO_JUGADOR FROM JUGADORES
WHERE (POSICION LIKE'DEL%');

#----PUNTO 5----#
SELECT CODIGO_EQUIPO,NOMBRE_EQUIPO, NOMBRE_ESTADIO,AFORO,ANIO_FUNDACION,CIUDAD_EQUIPO FROM EQUIPOS
WHERE (CIUDAD_EQUIPO LIKE'TUN%');

#----PUNTO 6----#
SELECT CODIGO_EQUIPO,NOMBRE_EQUIPO, NOMBRE_ESTADIO,AFORO,ANIO_FUNDACION,CIUDAD_EQUIPO FROM EQUIPOS
WHERE (NOMBRE_ESTADIO LIKE'LA%');

#----PUNTO 7----#
SELECT *FROM PARTIDOS;
UPDATE PARTIDOS SET FECHA_PARTIDO='2018-10-29' WHERE CODIGO_PARTIDO=1;

#----PUNTO 9----#
SELECT NOMBRE_EQUIPO,CIUDAD_EQUIPO , NOMBRE_ESTADIO FROM EQUIPOS;

#----PUNTO 10---#
SELECT NOMBRE_EQUIPO,DESCRIPCION,MINUTO_GOL,NOMBRE_JUGADOR FROM EQUIPOS
INNER JOIN GOLES,JUGADORES 
WHERE EQUIPOS.CODIGO_EQUIPO=GOLES.CODIGO_GOLES AND CODIGO_JUGADOR=CODIGO_GOLES;










