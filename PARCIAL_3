CREATE DATABASE DIMAYOR;
USE DIMAYOR;

CREATE TABLE JUGADORES (
    CODIGO_JUGADOR INT(55) NOT NULL AUTO_INCREMENT,
    NOMBRE_JUGADOR VARCHAR(25) NOT NULL,
    FECHA_NACIMIENTO_JUGADOR DATE,
    POSICION VARCHAR(25),
    CODIGO_EQUIPO INT(22),
    PRIMARY KEY (CODIGO_JUGADOR)
);
##   DROP TABLE EQUIPOS
CREATE TABLE EQUIPOS (
    CODIGO_EQUIPO INT(22) NOT NULL AUTO_INCREMENT,
    NOMBRE_EQUIPO VARCHAR(225) NOT NULL,
    NOMBRE_ESTADIO VARCHAR(225) NOT NULL,
    AFORO INT(55),
    ANIO_FUNDACION DATE,
    CIUDAD_EQUIPO VARCHAR(55),
    PRIMARY KEY (CODIGO_EQUIPO)
);

CREATE TABLE PARTIDOS (
    CODIGO_PARTIDO INT(22) NOT NULL AUTO_INCREMENT,
    FECHA_PARTIDO DATE,
    GOLES_CASA INT(22),
    GOLES_VISITANTE INT(22),
    PRIMARY KEY (CODIGO_PARTIDO)
);

CREATE TABLE GOLES (
    CODIGO_GOLES INT(22) NOT NULL AUTO_INCREMENT,
    DESCRIPCION VARCHAR(55),
    MINUTO_GOL TIME,
    PRIMARY KEY (CODIGO_GOLES)
);
## DROP TABLE PRESIDENTES
CREATE TABLE PRESIDENTES (
    DNI INT(22) NOT NULL AUTO_INCREMENT,
    NOMBRE VARCHAR(55),
    FECHA_NACIMIENTO_PRESIDENTE DATE,
    ANIO_ELECCION DATE,
    CODIGO_EQUIPO INT(22),
    FOREIGN KEY (DNI)REFERENCES EQUIPOS(CODIGO_EQUIPO),
    PRIMARY KEY (DNI)
);






